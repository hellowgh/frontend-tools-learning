{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./dist/",
    "module": "ESNext",
    // 指定 ECMAScript 目标版本: 'ES3' (default), 'ES5', 'ES6'/'ES2015', 'ES2016', 'ES2017', or 'ESNEXT'
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "exclude": ["/node_modules"],
  "include": ["src"]
}
